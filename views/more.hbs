<main class="bg">
  <div class="instagram clearfix">
    {{#photos}}
      <blockquote class="instagram-media" data-instgrm-captioned data-instgrm-version="4"
      style=" background:#FFF; float:left; border:0; border-radius:3px;
       margin: 1px; max-width:329px; padding:0; width:250px; height:500px; width:-webkit-calc(100% - 2px); width:calc(100% - 2px);">
          <div style=" background:url(data:image/png;base64,);
           display:block; height:44px; margin:0 auto -44px; position:relative; top:-22px; width:44px;">
         </div>
          <a href='{{link}}' style=" color:#000; font-family:Arial,sans-serif; font-size:14px; font-style:normal; font-weight:normal; line-height:17px; text-decoration:none; word-wrap:break-word;" target="_top">
          </a>
    </blockquote>
    {{/photos}}
    <script async defer src="//platform.instagram.com/en_US/embeds.js"></script>
  </div>
</main>
