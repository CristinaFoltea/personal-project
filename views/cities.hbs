
<main class="bg">
  <article class='city-list'>
    <table>
      <tr class="title">
        <td>Destination</td>
        <td>Price</td>
        <td>Save</td>
        <td>See Instagram</td>
      </tr>
    <div class="result">
      {{#results}}
          <tr>
            <td class="city_name">{{city}}</td>
            <td class="price">${{price}}</td>
            <td><button type="button" id="{{city}}" onclick="save('{{city}}')">Save</button></td>
            <td><a href="/photos/{{city}}" class="button insta" id='{{code}}' target="_blank">See {{city}} on Instagram</a></td>
          </tr>
      {{/results}}
    </div>
   </table>
  </article>
</main>
